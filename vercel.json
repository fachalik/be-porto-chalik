{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],

  "rewrites": [
    { "source": "/routes/(.*)", "destination": "/routes" },
    { "source": "/public/(.*)", "destination": "/public" },
    { "source": "/Images/(.*)", "destination": "/Images" },
    { "source": "/config/(.*)", "destination": "/config" }
  ]
}
